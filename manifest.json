    "https://secure.gravatar.com/",
    "http://www.w3schools.com/",
    "http://www.twitter.com",
    "http://cdn.syndication.twimg.com/",
    "https://platform.twitter.com/",
    "https://instagramimages-a.akamaihd.net/",
    "https://api.instagram.com/",
